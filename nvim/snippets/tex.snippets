snippet beg "\begin{}"
	\begin{$1}$2
		$3
	\end{$1}

snippet enum "\begin{enumerate}"
	\begin{enumerate}
		\item $1
	\end{enumerate}

snippet item "\begin{itemize}"
	\begin{itemize}
		\item $1
	\end{itemize}

snippet equa "\begin{equation}"
	\begin{equation$1}
		$2
	\end{equation$1}

snippet frac "\frac{}{}"
	\frac{$1}{$2}
